{
	"name": "tempest.launcher",
	"private": true,
	"version": "1.0.0-dev",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"proto-generate": "rimraf src/lib/rpc/* && protoc --ts_out src/lib/rpc --ts_opt server_none --proto_path ../Tempest.Protocol ../Tempest.Protocol/**/*.proto && dprint fmt src/lib/rpc/**/*.ts",
		"tauri": "tauri",
		"dprint": "dprint",
		"fmt": "dprint fmt"
	},
	"dependencies": {
		"@fontsource-variable/montserrat": "^5.2.8",
		"@lucide/svelte": "^0.544.0",
		"@protobuf-ts/grpcweb-transport": "^2.11.1",
		"@protobuf-ts/runtime": "^2.11.1",
		"@protobuf-ts/runtime-rpc": "^2.11.1",
		"@tauri-apps/api": "^2.8.0",
		"@tauri-apps/plugin-dialog": "~2.4.0",
		"@tauri-apps/plugin-http": "~2.5.2",
		"@tauri-apps/plugin-opener": "^2.5.0",
		"@tauri-apps/plugin-os": "~2.3.1",
		"@tauri-apps/plugin-shell": "~2.3.1",
		"bits-ui": "^2.11.1",
		"runed": "^0.34.0"
	},
	"devDependencies": {
		"@protobuf-ts/plugin": "^2.11.1",
		"@protobuf-ts/protoc": "^2.11.1",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/vite": "^4.1.13",
		"@tauri-apps/cli": "^2.8.4",
		"dprint": "^0.50.2",
		"rimraf": "^6.0.1",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2",
		"vite": "^7.1.7"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"dprint",
			"esbuild"
		]
	},
	"packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
